[user]
    name = Shreyas
[apply]
	whitespace = warn
[alias]
    st = status
	changelog = log --pretty=oneline --abbrev-commit --graph
	ciamd = commit --amend
	credits = shortlog -sn --no-merges
   	df = diff --color-words
	di = diff --ignore-space-change
	goback = reset --hard
	ld = log --pretty=format:"%C(yellow)%h\\ %C(green)%ad%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --date=short --graph
	lg = log --graph --date=short --date-order
	ll = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --numstat
	ls = log --pretty=format:"%C(green)%h\\ %C(yellow)[%ad]%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --date=relative
	lt = log --graph --date=short --topo-order
	pick = add -p
	pul = pull --verbose
	pur = pull --rebase
	po = push origin
	rename = branch -m 
   	staged = diff --cached	
	unstage = reset HEAD
	what = whatchanged -p
[blame]
	date=short
[branch]
	autosetuprebase = always
[core]
	editor = vim
    excludesfile = ~/.gitignore
    autocrlf = false
    pager = less -R
[color]
	ui = always
	pager = true
[color "status"]
	untracked = yellow
	changed = red
	added = green
[format]
	pretty="%Cgreen%h %Cred%ar(%ad) %Cblue<%an> %C(yellow)%d %Creset%s"
